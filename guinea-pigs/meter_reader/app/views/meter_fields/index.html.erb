<h1>Listing meter_fields</h1>

<table>
  <tr>
    <th>Meter</th>
    <th>Name</th>
    <th>Data</th>
  </tr>

<% @meter_fields.each do |meter_field| %>
  <tr>
    <td><%=h meter_field.meter_id %></td>
    <td><%=h meter_field.name %></td>
    <td><%=h meter_field.data %></td>
    <td><%= link_to 'Show', meter_field_path(meter_field) %></td>
    <td><%= link_to 'Edit', edit_meter_field_path(meter_field) %></td>
    <td><%= link_to 'Destroy', meter_field, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New meter_field', new_meter_field_path %>
